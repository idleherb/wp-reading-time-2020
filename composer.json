{
    "name": "idleherb/wp-reading-time-2020",
    "description": "A basic reading time plugin for WordPress",
    "version": "0.0.1",
    "prefer-stable" : true,
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.6.2",
        "wp-coding-standards/wpcs": "^2.2",
        "phpcompatibility/phpcompatibility-wp": "^2.1",
        "phpunit/phpunit": "^5 || ^7",
        "brain/monkey": "2.*",
        "php-mock/php-mock-mockery": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "idleherb\\ReadingTime\\": "includes"
        }
    }
}
